<block tt:if="{{isShareShow}}"><view class="animated fadeIn faster"><view data-event-opts="{{[['tap',[['togle',['$event']]]]]}}" class="more-share-mask" bindtap="__e"></view><view class="more-share"><view class="more-share-title u-f-ajc">分享到</view><scroll-view class="more-share-body u-f-ajc" scroll-x><block tt:for="{{providerList}}" tt:for-item="val" tt:for-index="index" tt:key="index"><block><view class="more-share-item" hover-class="more-share-hover" data-event-opts="{{[['tap',[['share',['$0'],[[['providerList','',index]]]]]]]}}" bindtap="__e"><view class="{{((('icon iconfont u-f-ajc')+' '+('icon-'+val.zicon))+' '+('share-'+val.zclass))}}"></view><view>{{val.name}}</view></view></block></block></scroll-view><view class="more-share-bottom u-f-ajc" hover-class="more-share-hover" data-event-opts="{{[['tap',[['togle',['$event']]]]]}}" bindtap="__e">取消</view></view></view></block>