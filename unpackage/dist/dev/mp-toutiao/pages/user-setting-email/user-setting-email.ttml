<view><input class="uni-input common-input" type="password" placeholder="输入邮箱" data-event-opts="{{[['input',[['__set_model',['','email','$event',[]]]]]]}}" value="{{email}}" bindinput="__e"/><input class="uni-input common-input" type="password" placeholder="输入密码" data-event-opts="{{[['input',[['__set_model',['','Pw','$event',[]]]]]]}}" value="{{Pw}}" bindinput="__e"/><button class="{{(('user-setting-logout-btn ')+' '+((disable)?'user-set-btn-disable':''))}}" type="primary" disabled="{{disable}}" loading="{{loading}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">修改密码</button></view>